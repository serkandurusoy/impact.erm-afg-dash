{
  "private": true,
  "name": "server",
  "version": "1.0.0",
  "description": "Server API for data queries",
  "scripts": {
    "start": "yarn run dev",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "dev": "yarn run lint && PORT=8002 TZ=Etc/UTC NODE_ENV=development nodemon src/index.js --exec 'yarn run lint && babel-node'",
